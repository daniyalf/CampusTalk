@model IEnumerable<CampusChat.Models.Message>

@{
    ViewBag.Title = "Sent Messages";
}

<h2>Sent Messages</h2>

<table class="table">
    <tr>
        <th>
            Sent To
        <th>
        <th>
            Message
        </th>
        <th>
            @Html.Label("Sent Time")
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AspNetUser1.UserName)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.Content)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.SentTime)
            </td>
        </tr>
    }
    <div>
        @Html.ActionLink("Back to Inbox", "Index")
    </div>

</table>
